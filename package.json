{
  "name": "jsreport-cli",
  "version": "0.1.0",
  "description": "Command line interface for jsreport",
  "main": "index.js",
  "bin": {
    "jsreport": "cli.js"
  },
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "test:watch": "mocha test --watch",
    "prepublish": "in-publish && standard || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsreport/jsreport-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-cli/issues"
  },
  "license": "MIT",
  "keywords": [
    "jsreport",
    "cli",
    "commands",
    "jsreport commands"
  ],
  "author": "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
  "contributors": [
    "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
    "Jan Blaha"
  ],
  "files": [
    "lib",
    "shared",
    "studio",
    "index.js",
    "cli.js",
    "example.server.js",
    "example.config.json",
    "jsreport.config.js"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "dependencies": {
    "bluebird": "3.4.7",
    "ip-address": "5.8.6",
    "is-absolute-url": "2.1.0",
    "is-promise": "2.1.0",
    "jsreport-client": "0.4.1",
    "liftoff": "2.3.0",
    "map-async": "0.1.1",
    "npm-install-package": "2.1.0",
    "nssocket": "0.6.0",
    "object-assign": "4.1.1",
    "once": "1.4.0",
    "os-homedir": "1.0.2",
    "path-is-absolute": "1.0.1",
    "prompt": "1.0.0",
    "semver": "5.3.0",
    "shortid": "2.2.6",
    "silent-spawn": "0.2.0",
    "yargs": "6.6.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.14.1",
    "eslint-plugin-babel": "4.0.1",
    "in-publish": "2.0.0",
    "jsreport-core": "1.1.0",
    "mocha": "3.2.0",
    "should": "11.2.0",
    "standard": "8.6.0"
  },
  "optionalDependencies": {
    "winser": "1.0.2"
  }
}

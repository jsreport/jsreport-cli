{
  "name": "jsreport-cli",
  "version": "2.1.1",
  "description": "Command line interface for jsreport",
  "main": "index.js",
  "bin": {
    "jsreport": "cli.js"
  },
  "scripts": {
    "start": "cross-env JSREPORT_CLI_DEV=enabled node cli.js",
    "test": "mocha --recursive \"./test/*.spec.js\" \"./test/commands/**/*.spec.js\" --timeout 28000 --exit",
    "test:watch": "mocha --recursive \"./test/*.spec.js\" \"./test/commands/**/*.spec.js\" --timeout 28000 --exit --watch",
    "prepublish": "in-publish && standard || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsreport/jsreport-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-cli/issues"
  },
  "license": "MIT",
  "keywords": [
    "jsreport",
    "cli",
    "commands",
    "jsreport commands"
  ],
  "author": "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
  "contributors": [
    "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
    "Jan Blaha"
  ],
  "files": [
    "lib",
    "shared",
    "studio",
    "index.js",
    "cli.js",
    "test/testUtils.js",
    "example.server.js",
    "example.config.json",
    "jsreport.config.js"
  ],
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "dependencies": {
    "chalk": "2.4.2",
    "cliui": "4.1.0",
    "inquirer": "6.2.2",
    "ip-address": "5.8.9",
    "is-absolute-url": "2.1.0",
    "is-promise": "2.1.0",
    "jsreport-client": "1.0.2",
    "liftoff": "3.0.0",
    "lodash.omit": "4.5.0",
    "nanoid": "1.0.2",
    "npm-install-package": "2.1.0",
    "nssocket": "0.6.0",
    "once": "1.4.0",
    "prompt-tmp": "1.0.0",
    "semver": "5.6.0",
    "silent-spawn": "0.4.0",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "cross-env": "5.2.1",
    "eslint": "4.18.1",
    "eslint-plugin-babel": "4.1.2",
    "execa": "1.0.0",
    "handlebars": "4.4.3",
    "in-publish": "2.0.0",
    "jsreport-authentication": "2.3.0",
    "jsreport-core": "2.6.2",
    "jsreport-express": "2.5.0",
    "jsreport-fs-store": "2.6.0",
    "jsreport-handlebars": "2.1.0",
    "jsreport-templates": "2.3.2",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "node.extend.without.arrays": "1.1.6",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "sinon": "4.4.0",
    "standard": "11.0.0",
    "std-mocks": "1.0.1"
  },
  "optionalDependencies": {
    "winser-with-api": "1.0.1"
  },
  "engines": {
    "node": ">=8.9"
  }
}

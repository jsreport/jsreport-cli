{
  "name": "jsreport-cli",
  "version": "1.3.2",
  "description": "Command line interface for jsreport",
  "main": "index.js",
  "bin": {
    "jsreport": "cli.js"
  },
  "scripts": {
    "test": "mocha --recursive \"./test/commander.spec.js\" \"./test/cli/cli.spec.js\" \"./test/commands/**/*.spec.js\" --timeout 10000 --exit",
    "test-integration": "mocha --recursive \"./test/integration/**/*.spec.js\" --timeout 10000",
    "test:watch": "mocha --recursive \"./test/commander.spec.js\" \"./test/commands/**/*.spec.js\" --timeout --exit 7000 --watch",
    "prepublish": "in-publish && standard || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsreport/jsreport-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-cli/issues"
  },
  "license": "MIT",
  "keywords": [
    "jsreport",
    "cli",
    "commands",
    "jsreport commands"
  ],
  "author": "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
  "contributors": [
    "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
    "Jan Blaha"
  ],
  "files": [
    "lib",
    "shared",
    "studio",
    "index.js",
    "cli.js",
    "example.server.js",
    "example.config.json",
    "jsreport.config.js"
  ],
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "inquirer": "5.1.0",
    "ip-address": "5.8.9",
    "is-absolute-url": "2.1.0",
    "is-promise": "2.1.0",
    "jsreport-client": "jsreport/nodejs-client#2.0.0-beta",
    "liftoff": "2.5.0",
    "map-async": "0.1.1",
    "nanoid": "1.0.2",
    "npm-install-package": "2.1.0",
    "nssocket": "0.6.0",
    "object-assign": "4.1.1",
    "once": "1.4.0",
    "path-is-absolute": "1.0.1",
    "prompt-tmp": "1.0.0",
    "semver": "5.5.0",
    "silent-spawn": "0.4.0",
    "yargs": "6.6.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.18.1",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-core": "1.5.1",
    "mkdirp": "0.5.1",
    "mocha": "5.0.1",
    "rimraf": "2.6.2",
    "should": "13.2.1",
    "sinon": "4.4.0",
    "standard": "11.0.0",
    "std-mocks": "1.0.1"
  },
  "optionalDependencies": {
    "winser-with-api": "1.0.1"
  },
  "engines": {
    "node": ">=8.6"
  }
}
